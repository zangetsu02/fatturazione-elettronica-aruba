{
  "name": "@fatturazione-aruba/communications",
  "version": "0.1.0",
  "description": "Financial communications for Aruba Electronic Invoicing",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "aruba",
    "fatturazione-elettronica",
    "e-invoice",
    "sdi",
    "italy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fatturazione-aruba/core": "workspace:*"
  },
  "devDependencies": {
    "@fatturazione-aruba/typescript-config": "workspace:*"
  }
}
